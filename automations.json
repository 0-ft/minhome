{
  "automations": [
    {
      "id": "little-lamp-blink",
      "name": "Blink Little Lamp",
      "enabled": true,
      "triggers": [
        {
          "type": "interval",
          "every": 5
        }
      ],
      "conditions": [],
      "actions": [
        {
          "type": "conditional",
          "condition": {
            "type": "device_state",
            "device": "0x44e2f8fffe194350",
            "property": "state",
            "equals": "ON"
          },
          "then": [
            {
              "type": "device_set",
              "device": "0x44e2f8fffe194350",
              "payload": {
                "state": "OFF"
              }
            }
          ],
          "else": [
            {
              "type": "device_set",
              "device": "0x44e2f8fffe194350",
              "payload": {
                "state": "ON"
              }
            }
          ]
        }
      ]
    }
  ]
}
